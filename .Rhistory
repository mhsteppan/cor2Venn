cor2venn <- function(cormat, Rsquared = TRUE, Recode = TRUE)
{
x<-c("a","b","c")
plot <- ggplot(data=mtcars, aes(x=wt, y=mpg)) + geom_point()
result<-list(plot,x)
names(result)<-c("plot","x")
return(result)
}
